{
    "name": "KINGROUP",
    "description": "KINGROUP AUTOZOOM",
    "version": "0.1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "webRequestBlocking",
        "webRequest",
        "http://*/",
        "https://*/"
    ],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}